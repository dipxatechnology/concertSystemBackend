    },
    "date": {
      "type": "date",
      "required": true
    },
    "time": {
      "type": "time"
    },
    "venue": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": false
    },
    "country": {
      "type": "string",
