{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "Ticket",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "concert": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::concert.concert",
      "inversedBy": "tickets"
    },
    "app_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::app-user.app-user",
      "inversedBy": "tickets"
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "price": {
      "type": "decimal"
    },
    "payment_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "completed",
        "canceled",
        "unavailable"
      ],
      "default": "pending"
    },
    "purchase_date": {
      "type": "datetime"
    }
  }
}
