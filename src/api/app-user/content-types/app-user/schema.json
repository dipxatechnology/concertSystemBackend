    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "app_user"
    },
    "feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback",
      "mappedBy": "app_user"
    },
    "wishlists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wishlist.wishlist",
      "mappedBy": "app_user"
    }
  }
