{
  "kind": "collectionType",
  "collectionName": "artists",
  "info": {
    "singularName": "artist",
    "pluralName": "artists",
    "displayName": "Artist",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "artist_id": {
      "type": "integer",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "genre": {
      "type": "string"
    },
    "bio": {
      "type": "blocks",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "website": {
      "type": "string"
    },
    "socialMediaLinks": {
      "type": "string"
    },
    "concert_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::concert.concert",
      "inversedBy": "artist_ids"
    },
    "ticket": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ticket.ticket",
      "mappedBy": "artist"
    }
  }
}
